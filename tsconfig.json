{
	"include": ["./src/**/*"],
	"exlude": ["node_modules", ".cache"],
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["dom", "ESNext"],
		"allowJs": true,
		"jsx": "react",
		"strict": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noEmit": true,
		"skipLibCheck": true
	}
}
